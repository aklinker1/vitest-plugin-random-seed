{
  "name": "vitest-plugin-random-seed",
  "version": "1.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/aklinker1/vitest-plugin-random-seed"
  },
  "homepage": "https://github.com/aklinker1/vitest-plugin-random-seed",
  "author": {
    "name": "Aaron Klinker",
    "email": "aaronklinker1+npm@gmail.com"
  },
  "keywords": [
    "seed",
    "test",
    "vitest",
    "faker",
    "chance",
    "falso"
  ],
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "files": [
    "dist",
    "src/types.d.ts"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./types": {
      "types": "./src/types.d.ts"
    }
  },
  "scripts": {
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "build": "bunx --bun unbuild",
    "check": "bunx --bun check",
    "prepare": "simple-git-hooks",
    "prepublish": "bun run build"
  },
  "devDependencies": {
    "@aklinker1/check": "^1.0.3",
    "@types/bun": "latest",
    "@vitest/coverage-v8": "^1.3.1",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "publint": "^0.2.7",
    "simple-git-hooks": "^2.10.0",
    "typescript": "^5.0.0",
    "unbuild": "^2.0.0",
    "vite": "^5.1.6",
    "vitest": "^1.3.1"
  },
  "peerDependencies": {
    "vite": "^4.0.0 || ^5.0.0 || ^6.0.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write"
  },
  "changelog": {
    "excludeAuthors": [
      "aaronklinker1@gmail.com"
    ]
  }
}
